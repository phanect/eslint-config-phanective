{
  "name": "eslint-config-phanective",
  "version": "2023.8.26",
  "description": "Personal ESLint configuration for Jumpei Ogawa (@phanect)",
  "main": "./plain.json",
  "scripts": {
    "build": "node --trace-warnings --unhandled-rejections=strict ./scripts/build.js",
    "test": "jest",
    "lint": "npm run lint:eslint && npm run lint:dprint",
    "lint:eslint": "eslint --ignore-path=.gitignore --ext=.js,.cjs,.mjs,.jsx,.ts,.tsx,.vue,.json,.json5 .",
    "lint:dprint": "dprint check",
    "clean": "git clean -dX --force",
    "release": "npm run build && npm publish . --access public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:phanect/eslint-plugin.git"
  },
  "author": "Jumpei Ogawa",
  "license": "CC0-1.0",
  "engines": {
    "node": ">=10.x"
  },
  "dependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint-config-next": "latest",
    "eslint-plugin-document-write": "latest",
    "eslint-plugin-editorconfig": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-jsonc": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "eslint-plugin-vue": "latest",
    "lodash.clonedeep": "^4.5.0",
    "lodash.mergewith": "^4.6.2",
    "typescript": "latest"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "dprint": "^0.45.1",
    "jest": "^28.1.3"
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  }
}
