{
  "name": "eslint-config-phanective",
  "version": "2023.8.26",
  "description": "Personal ESLint configuration for Jumpei Ogawa (@phanect)",
  "main": "./plain.json",
  "type": "module",
  "scripts": {
    "build": "node --trace-warnings --unhandled-rejections=strict ./scripts/build.js",
    "test": "vitest",
    "lint": "tsc --noEmit && eslint --ignore-path=.gitignore --ext=.js,.cjs,.mjs,.jsx,.ts,.tsx,.vue,.json,.json5 .",
    "clean": "git clean -dX --force",
    "release": "npm run build && npm publish . --access public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:phanect/eslint-plugin.git"
  },
  "author": "Jumpei Ogawa (https://phanective.org)",
  "license": "CC0-1.0",
  "engines": {
    "node": ">=18.x",
    "npm": ">=10.x"
  },
  "dependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint-config-next": "latest",
    "eslint-plugin-document-write": "latest",
    "eslint-plugin-editorconfig": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-jsonc": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "eslint-plugin-vue": "latest"
  },
  "devDependencies": {
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/lodash.mergewith": "^4.6.9",
    "@types/node": "^20.10.6",
    "eslint": "^8.0.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.1.0"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "next": "*",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  }
}
