{
  "name": "eslint-config-phanective",
  "version": "2023.8.26",
  "description": "Personal ESLint configuration for Jumpei Ogawa (@phanect)",
  "type": "module",
  "main": "./plain.json",
  "scripts": {
    "build": "esno ./scripts/build.ts",
    "test": "npm run build && vitest run",
    "lint": "npm run build && tsc --noEmit && eslint --ext=.js,.cjs,.mjs,.jsx,.ts,.tsx,.vue,.json,.json5 .",
    "clean": "git clean -dX --force",
    "release": "npm run build && npm publish . --access public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:phanect/eslint-config-phanective.git"
  },
  "author": "Jumpei Ogawa (https://phanective.org)",
  "license": "CC0-1.0",
  "engines": {
    "node": ">=18.x",
    "npm": ">=10.x"
  },
  "dependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint-config-next": "latest",
    "eslint-import-resolver-typescript": "latest",
    "eslint-plugin-document-write": "latest",
    "eslint-plugin-editorconfig": "latest",
    "eslint-plugin-import-x": "^0.5.0",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-jsonc": "latest",
    "eslint-plugin-n": "^17.7.0",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "latest"
  },
  "devDependencies": {
    "@phanect/utils": "^1.0.0",
    "@types/node": "^20.10.6",
    "deepmerge": "^4.3.1",
    "eslint": "^8.0.0",
    "esno": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "next": "*",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  }
}
