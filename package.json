{
  "name": "eslint-config-phanective",
  "version": "2024.5.27",
  "description": "Personal ESLint configuration for Jumpei Ogawa (@phanect)",
  "type": "module",
  "module": "./dist/eslint.mjs",
  "types": "./dist/eslint.d.ts",
  "exports": {
    "./eslint": {
      "import": {
        "default": "./dist/eslint.mjs",
        "types": "./dist/eslint.d.ts"
      }
    }
  },
  "scripts": {
    "build": "unbuild",
    "test": "npm run build && vitest run",
    "lint": "npm run build && tsc --noEmit && eslint",
    "fix": "npm run build && eslint --fix",
    "rwt": "esno ./scripts/rwt.ts",
    "clean": "git clean -dX --force",
    "release": "npm run build && npm publish . --access public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:phanect/eslint-config-phanective.git"
  },
  "author": "Jumpei Ogawa (https://phanective.org)",
  "license": "CC0-1.0",
  "engines": {
    "node": ">=18.x",
    "npm": ">=10.x"
  },
  "dependencies": {
    "@eslint/js": "^9.7.0",
    "@nuxt/eslint-plugin": "^0.3.13",
    "eslint-plugin-document-write": "^1.0.1",
    "eslint-plugin-editorconfig": "^4.0.3",
    "eslint-plugin-jsdoc": "^48.2.5",
    "eslint-plugin-jsonc": "^2.15.1",
    "eslint-plugin-n": "^17.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-svelte": "^2.39.0",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^9.26.0",
    "typescript-eslint": "^8.0.0-alpha.51"
  },
  "devDependencies": {
    "@phanect/configs": "latest",
    "@phanect/utils": "^1.0.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.10.6",
    "deepmerge": "^4.3.1",
    "eslint": "^9.7.0",
    "eslint-flat-config-utils": "^0.2.5",
    "esno": "^4.7.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitest": "^2.0.2"
  },
  "peerDependencies": {
    "eslint": "^9.7.0",
    "next": "*",
    "svelte": "*",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  }
}
